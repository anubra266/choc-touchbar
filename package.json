{
  "name": "choc-touchbar",
  "displayName": "Choc Touchbar",
  "description": "My preferred vscode content for MacBook Touchbar",
  "version": "0.0.1",
  "publisher": "anubra266",
  "engines": {
    "vscode": "^1.52.0"
  },
  "repository": "https://github.com/anubra266/choc-touchbar",
  "categories": [
    "Other"
  ],
  "keywords": [
    "macbook pro",
    "touchbar",
    "chakra",
    "chakra ui",
    "shortcuts",
    "vscode",
    "visual studio code",
    "macbook",
    "developers",
    "touch bar"
  ],
  "activationEvents": [
    "onCommand:choc-touchbar.git",
    "onCommand:choc-touchbar.gitcommit",
    "onCommand:choc-touchbar.gitpush",
    "onCommand:choc-touchbar.chakra",
    "onCommand:choc-touchbar.chakradocs",
    "onCommand:choc-touchbar.closegit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "choc-touchbar.git",
        "title": "Git",
        "icon": "./assets/git.png"
      },
      {
        "command": "choc-touchbar.gitcommit",
        "title": "üìò Commit"
      },
      {
        "command": "choc-touchbar.gitpush",
        "title": "üöÄ Push"
      },
      {
        "command": "choc-touchbar.closegit",
        "title": "‚Üê "
      },
      {
        "command": "choc-touchbar.chakra",
        "title": "Chakra UI",
        "icon": "./assets/chakra.png"
      },
      {
        "command": "choc-touchbar.chakradocs",
        "title": "üìì Docs"
      },
      {
        "command": "choc-touchbar.closechakra",
        "title": "‚Üê "
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "choc-touchbar.git",
          "group": "gittools",
          "when": "!enableGit && !enableChakra"
        },
        {
          "command": "choc-touchbar.gitcommit",
          "group": "git-action",
          "when": "enableGit"
        },
        {
          "command": "choc-touchbar.gitpush",
          "group": "git-action",
          "when": "enableGit"
        },
        {
          "command": "choc-touchbar.closegit",
          "group": "git-action",
          "when": "enableGit"
        },
        {
          "command": "choc-touchbar.chakra",
          "group": "tools",
          "when": "!enableGit && !enableChakra"
        },
        {
          "command": "choc-touchbar.chakradocs",
          "group": "tools",
          "when": "enableChakra"
        },
        {
          "command": "choc-touchbar.closechakra",
          "group": "tools",
          "when": "enableChakra"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "icon": "logo.png",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
